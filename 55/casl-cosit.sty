%%\setlength{\abovecaptionskip}{0mm}
\newcommand\floatc@specplain[2]{\setbox\@tempboxa\hbox{{\bf #1:} #2}%
  \ifdim\wd\@tempboxa>\linewidth \hbox to\linewidth{%
    \hfil\parbox{\floatcwidth}{{\bf #1:} #2}\hfil}
    \else\hbox to\linewidth{\hfil\box\@tempboxa\hfil}\fi}
\newcommand\fs@specruled{\def\@fs@cfont{\bfseries}\let\@fs@capt\floatc@specplain
  \def\@fs@pre{\relax}%
  \def\@fs@post{\kern2pt\rule{1em}{.0pt}\rule{\floatcwidth}{0.4pt}\kern2pt\relax}%
  \def\@fs@mid{\kern2pt\rule{1em}{.0pt}\rule{\floatcwidth}{0.4pt}\kern2pt}%
  \let\@fs@iftopcapt\iftrue}
\newcommand\fs@spectable{\def\@fs@cfont{\bfseries}\let\@fs@capt\floatc@specplain
  \def\@fs@pre{}\def\@fs@post{}%
  \def\@fs@mid{\vspace\abovecaptionskip\relax}%
  \let\@fs@iftopcapt\iftrue}
\newcommand\fs@specfigure{\def\@fs@cfont{\bfseries}\let\@fs@capt\floatc@specplain
  \def\@fs@pre{}\def\@fs@post{}%
  \def\@fs@mid{\vspace\abovecaptionskip\relax}%
  \let\@fs@iftopcapt\iffalse}
\newenvironment{legende}[1][50mm]{\begin{center}\ifdim\wd\@tempboxa > \floatcwidth%
    \begin{minipage}{\floatcwidth}\small%
      \else  \begin{minipage}{#1}\small\fi}
  {\end{minipage}\end{center}}
